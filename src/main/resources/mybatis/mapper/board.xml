<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.babpool.rice.dao.IBoardDao">
	<select id="getBoardList" resultType="com.babpool.rice.vo.BoardVO">
		SELECT	
			 board_no
			 ,board_title
			 ,board_author
		FROM boards
	
	</select>

	
