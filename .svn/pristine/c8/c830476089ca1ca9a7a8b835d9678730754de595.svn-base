<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.babpool.faq.dao.IFaqDao">
	
	
	<select id="getFaqList" resultType="com.babpool.faq.vo.FaqVO">
		SELECT
		    faq_no
		    , faq_question
		    , faq_answer
		FROM
		    faq
		ORDER BY faq_no DESC
	</select>
	
</mapper>