@font-face {
    font-family: 'InfinitySans-RegularA1';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_20-04@2.1/InfinitySans-RegularA1.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}

/* General CSS Setup */
* {
    margin: 0 auto;
    padding: 0;
    list-style: none;
    text-decoration: none;
    color: black;
	font-family: 'InfinitySans-RegularA1';
}
::-webkit-scrollbar {
    width: 15px;
}
::-webkit-scrollbar-button {
    width: 15px;
    height: 5px;
}
::-webkit-scrollbar-track {
    background: #CCCCCC;
    border: thin solid lightgray;
    box-shadow: 0px 0px 3px #dfdfdf inset;
    border-radius:10px;
}
::-webkit-scrollbar-thumb {
    background:#666666;
    border: thin solid #999999;
    border-radius: 10px;
}
::-webkit-scrollbar-thumb:hover {
    background:#666666;
}  
    
  /* container */
  .container {
    padding: 5% 5%;
  }
  
  /* CSS talk bubble */
  .talk-bubble {
     margin: 10px;
    display: inline-block;
    position: relative;
      width: auto;
      height: auto;
      background-color: #e1e3e9;
  }
  .round{
    border-radius: 30px;
      -webkit-border-radius: 30px;
      -moz-border-radius: 30px;
  }

  #scrollDiv{
      width: 800px;
      height: 600px;
      z-index: 10;
      overflow: auto;
      border-radius: 15px;
  }

  #roomTitle{
      text-align: center;
      padding-top: 10px;
  		font-size: 20px;
  }

  #roomMaster{
      text-align: center;
      padding: 10px;
      font-size: 15px;
  }
  
  #roomPrice{
  	  text-align: center;
      padding: 10px;
      font-size: 15px;
  }
  
  #chatHr{
      padding: 5px;
  }
  
  #roominfo {
  	background-color: #e1e3e9;
  	height: 200px;
  	text-align: center;
  }

  /* masterChat */
  #masterChat{
    float: left;
    clear: both;
  }

  #masterChat > #userImg{
    width: 50px;
    height: 50px;
    border-radius: 50px;
  }
   
  /* myChat */
  #myChat{
    float: right;
    clear: both;
    margin-right: 20px;
  }

  #myChat > #userImg{
    width: 50px;
    height: 50px;
    border-radius: 50px;
  }
  
  /* userChat */
  #userChat{
  	 margin-left: 20px;
    float: left;
    clear: both;
  }

  #userChat > #userImg{
    width: 50px;
    height: 50px;
    border-radius: 50px;
  }
  
  /* chatInfo */
  .talktext{
    border-radius: 10px;
  }
  
  .talktext > p{
    text-align: center;
  }
  
   /* enter, exit, blank */
  #sendEnter{
    clear: both;
    text-align: center;
    padding-bottom: 10px;
    font-size: 11px;
  }
  #sendExit{
    clear: both;
    text-align: center;
    padding-bottom: 10px;
    font-size: 11px;
  }

  #sendBlank{
    clear: both;
    text-align: center;
    padding-bottom: 10px;
  }

  #sendBlank > h2{
    color: red;
  }
  
  #sendBlank > p{
    font-size: x-large;
    color: tomato;
  }
  
