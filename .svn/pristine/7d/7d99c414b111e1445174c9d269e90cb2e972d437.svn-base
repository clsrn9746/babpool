body{
    padding: 0;
    margin: 0;
    overflow-x: hidden;
    overflow-y: hidden;
  }
 div{
    box-sizing: border-box;
  }
  
  .summaryContainer{
    background-color: white;  
    display: flex;  
    padding: 21px 16px; 
    width: 500px; 
    height: 500px;
    margin-bottom: 10px;
  }

  .summaryContainer:nth-child(3) .title{
    font-size: 20px;
  }
  
  .summaryContainer .profiles  {
    font-size: 15px;
    margin-right: 20px;
    flex-grow: 1
  }
  
.title{
    font-size: 30px;
    font-weight: bold;
    margin-bottom: 15px;
    text-align: center;
  }
.table{
    text-align: center;
}
  

.box {
    position: inherit;
    bottom: 60px;
    left: 20px;
    width: 200px;
    height: 200px; 
    border-radius: 20%;
    overflow: hidden;
    border: solid white 3px;
    display: inline-table;
}
.profile {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.name{
    position: absolute;
    left: 270px;
    top: 50px;
    font-size: 30px;
}


.profileimg{
